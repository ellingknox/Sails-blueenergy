  <body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
  <!-- Intro Header -->
  <header class="intro custom-intro">
    <div class="background-cover">
      <div style="position: absolute; top:45px; left: 80px;z-index: 2;"><a href="/" class="link-home"><i class="fa fa-long-arrow-left"></i> Home</a></div>
      <!-- <div style="position: absolute;margin: 0 auto;width:100%; height: 100%; opacity: 0.5;"><img src="img/compare.png"></div> -->
      <div class="intro-body">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div id="login_main">
                <div class="col-xs-4 col-xs-offset-4 ">
                  <img src="img/heat-engine1.png" width="150px" style="margin-top:85px;">
                </div>
                <div class="col-xs-12"><h1 class="brand-heading">WELCOME TO BLUE ENERGY COIN</h1></div>
                <div class="col-xs-12 alerterror">
                  <span id="alerterror">
                    <% if (req.session.flash && req.session.flash.error) { %>
                        <%= req.flash('error') %>
                    <% } %>
                  </span>
                </div>
                <div class="col-xs-12">
                  <form id="log-form" action="/login" method="post" role="form">
                    <div class="form-content">
                      <div class="lavel-icon" style="margin-left: 14px;
                        margin-top: 5px;"><img src="img/mail.png"></div>
                      <input type="email" name="email" id="log-email" tabindex="1" class="log-input" placeholder="E-mail" value="" required>
                    </div>
                    <div class="form-content ">
                      <div class="lavel-icon"><img src="img/lock.png"></div>
                      <input type="password" name="password" id="log-password" tabindex="2" class="log-input" placeholder="Password" value="" required>
                    </div>
                    <div class="form-submit">
                      <input type="submit" name="login-submit" id="login-submit" tabindex="3" class="btn2" value="LOG IN">
                    </div>
                  </form>
                </div>
                <div class="col-xs-12">
                  <p class="log-link-text">Don't have an account? <a href="/signup">Sign up</a></p>
                </div>
                <div class="col-xs-12">
                  <a href="#" class="log-pass-text">Forgot your password?</a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>
  <!-- jQuery -->
  <!-- <script src="vendor/jquery/jquery.js"></script> -->

  <!-- Bootstrap Core JavaScript -->
  <script src="build/js/jquery-latest.min.js"></script>
