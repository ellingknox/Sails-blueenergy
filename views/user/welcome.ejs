    <script type="text/javascript">
        $(document).ready(function(){
            $('.dropdown-btn').click(function(){
                $('#side-menu').slideToggle("slow");
            });
            $(window).resize(function(){
                if ($(window).width() > 768) {
                    $('#side-menu').show();
                }
            });
            //Active control in Choose Payment
            $('.currency-link').click(function() {
              $('.currency-link').removeClass('active');
              $('.btn-copy').removeClass('copied');
              $(this).addClass('active');
              var coin = $(this).attr("href");
              $('#qrcode').children().remove();
              if (coin == 'bitcoin') {
                $('.source').text('1LFH5zrPi2i13yGfaahgfAZJYHtECcpS5U');
                $('#qrcode').qrcode({
                    width: 200,
                    height: 200,
                    text: "1LFH5zrPi2i13yGfaahgfAZJYHtECcpS5U"
                });

              }else if (coin == 'ethereum') {
                $('.source').text('0x5FC9bEd3Ddde4a05bFAA8638226dC93660f32B6A');
                $('#qrcode').qrcode({
                    width: 200,
                    height: 200,
                    text: "0x5FC9bEd3Ddde4a05bFAA8638226dC93660f32B6A"
                });
              }
              return false;
            });
            $('.btn-copy').click(function(){
              $(this).addClass('copied');
                return false;
            });

            $('#qrcode').qrcode({
                width: 200,
                height: 200,
                text: "1LFH5zrPi2i13yGfaahgfAZJYHtECcpS5U"
            });
        });
        function copyToClipboard() {
          var $temp = $("<input>");
          $("body").append($temp);
          $temp.val($('.source').text()).select();
          document.execCommand("copy");
          $temp.remove();
        }
    </script>
    
