            plot1 = $.jqplot('chart', [s1], {
                animate: !$.jqplot.use_excanvas,
                seriesDefaults:{
                    renderer:$.jqplot.BarRenderer,
                    rendererOptions: {
                    },
                    shadow: false,
                    showMarker: false,
                    pointLabels: { show: true },
                    color: '#1f88a7',
                },
                grid: {
                    background: 'rgba(57,57,57,0.0)',
                    drawBorder: false,
                    shadow: false,
                    gridLineColor: '#f0f0f0',
                    drawGridLines: false
                },
                legend: {
                    show: false,
                },
                axes: {
                    xaxis: {
                        renderer: $.jqplot.CategoryAxisRenderer,
                        ticks: ticks,
                        pad: 0,
                    },
                    yaxis: {
